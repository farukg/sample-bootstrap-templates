<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <sample-bootstrap-templates j:dependencies="bootstrap bootstrap-components circular-navigation contact counter default event facets search teasers usermessages"
                             j:installedModules="sample-bootstrap-templates"
                                  j:modulePriority="0"
                             j:moduleType="templatesSet"
                             j:title="Sample Bootstrap Templates"
                                  jcr:mixinTypes="jmix:hasExternalProviderExtension"
                             jcr:primaryType="jnt:module">
                
                <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder">
                    <bootstrap jcr:primaryType="jnt:folder">
                        <css jcr:primaryType="jnt:folder">
                            <bootstrap.css jcr:primaryType="jnt:file">
                                <jcr:content jcr:mimeType="text/css" jcr:primaryType="jnt:resource"/>
                            </bootstrap.css>
                        </css>
                        <img jcr:primaryType="jnt:folder">
                            <glyphicons-halflings.png j:height="159"
                                            j:width="469"
                                            jcr:mixinTypes="jmix:image"
                                            jcr:primaryType="jnt:file">
                                <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                                <thumbnail jcr:mimeType="application/binary" jcr:primaryType="jnt:resource"/>
                                <thumbnail2 jcr:mimeType="application/binary" jcr:primaryType="jnt:resource"/>
                            </glyphicons-halflings.png>
                            <body-bg.png j:height="400"
                               j:width="400"
                               jcr:mixinTypes="jmix:image"
                               jcr:primaryType="jnt:file">
                                <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                                <thumbnail jcr:mimeType="application/binary" jcr:primaryType="jnt:resource"/>
                                <thumbnail2 jcr:mimeType="application/binary" jcr:primaryType="jnt:resource"/>
                            </body-bg.png>
                            <glyphicons-halflings-white.png j:height="159"
                                                  j:width="469"
                                                  jcr:mixinTypes="jmix:image"
                                                  jcr:primaryType="jnt:file">
                                <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                                <thumbnail jcr:mimeType="application/binary" jcr:primaryType="jnt:resource"/>
                                <thumbnail2 jcr:mimeType="application/binary" jcr:primaryType="jnt:resource"/>
                            </glyphicons-halflings-white.png>
                            <logo.png j:height="100"
                            j:width="200"
                            jcr:mixinTypes="jmix:image"
                            jcr:primaryType="jnt:file">
                                <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                                <thumbnail jcr:mimeType="application/binary" jcr:primaryType="jnt:resource"/>
                                <thumbnail2 jcr:mimeType="application/binary" jcr:primaryType="jnt:resource"/>
                            </logo.png>
                        </img>
                    </bootstrap>
                </files>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <templates jcr:primaryType="jnt:templatesFolder">
                    <files jcr:primaryType="jnt:folder">
                        <bootstrap.png j:height="16"
                              j:width="16"
                              jcr:mixinTypes="jmix:image"
                              jcr:primaryType="jnt:file">
                            <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                            <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                        </bootstrap.png>
                    </files>
                    <contents jcr:primaryType="jnt:contentFolder"/>
                    <base j:view="bootstrap" jcr:primaryType="jnt:template">
                        <footer jcr:primaryType="jnt:contentList">
                            <footer-container jcr:mixinTypes="jmix:bootstrapFullComponentRowPredefined"
                                    jcr:primaryType="jnt:bootstrapFullComponent"
                                    layout="12"
                                    rowType="predefined">
                                <footer-container jcr:primaryType="jnt:contentList">
                                    <counternode jcr:primaryType="jnt:counterNode"/>
                                </footer-container>
                            </footer-container>
                        </footer>
                        <_x0033_col jcr:primaryType="jnt:pageTemplate">
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="3 Columns"/>
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <pagecontent jcr:mixinTypes="jmix:bootstrapFullComponentRowPredefined"
                                  jcr:primaryType="jnt:bootstrapFullComponent"
                                  layout="3_6_3"
                                  rowType="predefined">
                                    <pagecontent-side1 jcr:primaryType="jnt:contentList">
                                            <navigation-menu j:baselineNode="home"
                                               j:maxDepth="2"
                                               j:menuItemView="menuElement"
                                               j:startLevel="1"
                                               j:view="side"
                                               jcr:mixinTypes="jmix:renderable"
                                               jcr:primaryType="jnt:navMenu"/>
                                        <absolute-content j:level="1" jcr:primaryType="jnt:absoluteArea"/>
                                        <contextual-content jcr:primaryType="jnt:area"/>
                                        </pagecontent-side1>
                                    <pagecontent jcr:primaryType="jnt:contentList">
                                        <bootstrap-breadcrumb displayCurrentPage="true"
                                                 displayHome="true"
                                                 displayLinkOnCurrentPage="false"
                                                 displayOnFirstLevel="false"
                                                 divider="/"
                                                 jcr:primaryType="jnt:bootstrapBreadcrumb"/>
                                        <page-title hideInHomePage="false" jcr:primaryType="jnt:pageTitle"/>
                                        <pagecontent jcr:primaryType="jnt:area"/>
                            </pagecontent>
                            </pagecontent>
                            </pagecontent>
                        </_x0033_col>
                        <_x0031_col jcr:primaryType="jnt:pageTemplate">
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="1 Column"/>
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <pagecontent jcr:mixinTypes="jmix:bootstrapFullComponentRowPredefined"
                                  jcr:primaryType="jnt:bootstrapFullComponent"
                                  layout="12"
                                  rowType="predefined"/>
                            </pagecontent>
                            <information j:hiddenTemplate="false" jcr:primaryType="jnt:pageTemplate">
                                <j:translation_de jcr:language="de"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="information"/>
                                <pagecontent jcr:primaryType="jnt:contentList">
                                    <circularnavigation jcr:primaryType="sysewl:circularNavigation"/>
                                    <bootstrap-container fluid="false"
                                             j:expiration="0"
                                             j:perUser="false"
                                             jcr:mixinTypes="jmix:cache"
                                             jcr:primaryType="jnt:bootstrapContainer"/>
                                </pagecontent>
                            </information>
                        </_x0031_col>
                        <_x0032_cols j:hiddenTemplate="false" jcr:primaryType="jnt:pageTemplate">
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="2 columns"/>
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <pagecontent jcr:primaryType="jnt:contentList">
                                    <pagecontent jcr:mixinTypes="jmix:bootstrapFullComponentRowPredefined"
                                     jcr:primaryType="jnt:bootstrapFullComponent"
                                     layout="9_3"
                                     rowType="predefined">
                                        <pagecontent-side1 jcr:primaryType="jnt:contentList">
                                            <navigation-menu j:baselineNode="home"
                                               j:maxDepth="2"
                                               j:menuItemView="menuElement"
                                               j:startLevel="1"
                                               j:view="side"
                                               jcr:mixinTypes="jmix:renderable"
                                               jcr:primaryType="jnt:navMenu"/>
                                            <absolute-content j:level="1" jcr:primaryType="jnt:absoluteArea"/>
                                            <contextual-content jcr:primaryType="jnt:area"/>
                                        </pagecontent-side1>
                                        <pagecontent jcr:primaryType="jnt:contentList">
                                            <bootstrap-breadcrumb displayCurrentPage="true"
                                                    displayHome="true"
                                                    displayLinkOnCurrentPage="false"
                                                    displayOnFirstLevel="false"
                                                    divider="/"
                                                    jcr:primaryType="jnt:bootstrapBreadcrumb"/>
                                            <page-title hideInHomePage="true" jcr:primaryType="jnt:pageTitle"/>
                                            <pagecontent jcr:primaryType="jnt:area"/>
                                                </pagecontent>
                                            </pagecontent>
                                        </pagecontent>
                                    </pagecontent>
                        </_x0032_cols>
                        <navBar jcr:primaryType="jnt:contentList">
                            <bootstrap-navigation-menu j:maxDepth="2"
                                             j:menuItemView="menuDropdown"
                                             j:startLevel="0"
                                             jcr:primaryType="jnt:bootstrapNavMenu"
                                             position="right"/>
                        </navBar>
                        <bootstrap-header jcr:primaryType="jnt:contentList">
                            <bootstrap-layer-container-rows-a columns="span2, span6, span2, span1, span1"
                                                    jcr:mixinTypes="jmix:bootstrapFullComponentRowCustom"
                                                    jcr:primaryType="jnt:bootstrapFullComponent"
                                                    rowType="custom">
                                <bootstrap-layer-container-rows-a-0 jcr:primaryType="jnt:contentList">
                                    <image-from-the-document-manager j:linkType="none" jcr:primaryType="jnt:imageReferenceLink">
                                        <j:translation_en j:node="/modules/sample-bootstrap-templates/files/bootstrap/img/logo.png"
                                             jcr:language="en"
                                             jcr:primaryType="jnt:translation"/>
                                    </image-from-the-document-manager>
                                </bootstrap-layer-container-rows-a-0>
                                <bootstrap-layer-container-rows-a-1 jcr:primaryType="jnt:contentList">
                                    <navigation-menu j:maxDepth="1"
                                         j:menuItemView="menuElement"
                                         j:startLevel="0"
                                         jcr:primaryType="jnt:bootstrapNavMenu">
                                        <j:translation_en jcr:language="en"
                                             jcr:mixinTypes="mix:title"
                                             jcr:primaryType="jnt:translation"
                                             jcr:title="Navigation Menu"/>
                                    </navigation-menu>
                                </bootstrap-layer-container-rows-a-1>
                                <bootstrap-layer-container-rows-a-2 jcr:primaryType="jnt:contentList">
                                    <bootstrapmenuloginform j:view="menu"
                                                jcr:mixinTypes="jmix:renderable"
                                                jcr:primaryType="jnt:bootstrapMenuLoginForm"
                                                position="right"/>
                                    <or-register divClass="pull-right"
                                     j:invertCondition="true"
                                     j:requireLoggedUser="true"
                                     j:requirePrivilegedUser="false"
                                     jcr:mixinTypes="jmix:requiredPermissions"
                                     jcr:primaryType="jnt:siteLink"
                                     targetPageName="register">
                                        <j:translation_en jcr:language="en"
                                             jcr:mixinTypes="mix:title"
                                             jcr:primaryType="jnt:translation"
                                             jcr:title="or register"/>
                                    </or-register>
                                </bootstrap-layer-container-rows-a-2>
                                <bootstrap-layer-container-rows-a-3 jcr:primaryType="jnt:contentList">
                                    <account divClass="accountLink"
                                 j:invertCondition="false"
                                 j:requireLoggedUser="true"
                                 j:requirePrivilegedUser="false"
                                 jcr:mixinTypes="jmix:requiredPermissions"
                                 jcr:primaryType="jnt:siteLink"
                                 targetPageName="account">
                                        <j:translation_en jcr:language="en"
                                             jcr:mixinTypes="mix:title"
                                             jcr:primaryType="jnt:translation"
                                             jcr:title="Account"/>
                                    </account>
                                </bootstrap-layer-container-rows-a-3>
                                <bootstrap-layer-container-rows-a-4 jcr:primaryType="jnt:contentList">
                                    <messages divClass="messagesLink"
                                  j:invertCondition="false"
                                  j:requireLoggedUser="true"
                                  j:requirePrivilegedUser="false"
                                  jcr:mixinTypes="jmix:requiredPermissions"
                                  jcr:primaryType="jnt:siteLink"
                                  targetPageName="messages">
                                        <j:translation_en jcr:language="en"
                                             jcr:mixinTypes="mix:title"
                                             jcr:primaryType="jnt:translation"
                                             jcr:title="Messages"/>
                                    </messages>
                                </bootstrap-layer-container-rows-a-4>
                            </bootstrap-layer-container-rows-a>
                        </bootstrap-header>
                        <detail j:applyOn="jnt:article jnt:event jnt:news jnt:press"
                       j:defaultTemplate="true"
                       j:hiddenTemplate="true"
                       j:priority="150"
                       jcr:primaryType="jnt:contentTemplate">
                            <pagecontent jcr:primaryType="jnt:contentList">
                                <pagecontent jcr:mixinTypes="jmix:bootstrapFullComponentRowPredefined"
                                  jcr:primaryType="jnt:bootstrapFullComponent"
                                  layout="3_9"
                                  rowType="predefined">
                                    <pagecontent-side1 jcr:primaryType="jnt:contentList">
                                        <navigation-menu j:baselineNode="home"
                                            j:maxDepth="2"
                                            j:menuItemView="menuElement"
                                            j:startLevel="1"
                                            j:view="side"
                                            jcr:mixinTypes="jmix:renderable"
                                            jcr:primaryType="jnt:navMenu"/>
                                        <absolute-content j:level="1" jcr:primaryType="jnt:absoluteArea"/>
                                    </pagecontent-side1>
                                    <pagecontent jcr:primaryType="jnt:contentList">
                                        <bootstrap-breadcrumb displayCurrentPage="true"
                                                 displayHome="true"
                                                 displayLinkOnCurrentPage="false"
                                                 displayOnFirstLevel="false"
                                                 divider="/"
                                                 jcr:primaryType="jnt:bootstrapBreadcrumb"/>
                                        <pagecontent jcr:primaryType="jnt:area">
                                            <pagecontent jcr:primaryType="jnt:contentList">
                                                <main-resource-display j:mainResourceView="detail" jcr:primaryType="jnt:mainResourceDisplay"/>
                            </pagecontent>
                                </pagecontent>
                            </pagecontent>
                            </pagecontent>
                            </pagecontent>
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Detailed template"/>
                        </detail>
                    </base>
                        <site-settings-base j:view="siteSettings"
                                            jcr:mixinTypes="jmix:useUILocale"
                                            jcr:primaryType="jnt:template">
                            <bootstrapCustomization j:applyOn="jnt:virtualsite"
                                                    j:defaultTemplate="false"
                                                    j:hiddenTemplate="true"
                                                    j:icon="/modules/sample-bootstrap-templates/templates/files/bootstrap.png"
                                                    j:priority="101"
                                                    j:requiredPermissionNames="adminBootstrapCustomization"
                                                    j:titleKey="siteSettings.label.bootstrap"
                                                    jcr:mixinTypes="jmix:rbTitle"
                                                    jcr:primaryType="jnt:contentTemplate">
                                <pagecontent jcr:primaryType="jnt:contentList">
                                    <editLessVariables formview="sample" jcr:primaryType="jnt:editLessVariables"/>
                                </pagecontent>
                            </bootstrapCustomization>
                            <customization j:applyOn="jnt:virtualsite"
                                           j:defaultTemplate="false"
                                 j:hiddenTemplate="true"
                                 j:icon="/modules/sample-bootstrap-templates/templates/files/bootstrap.png"
                                 j:priority="100"
                                 j:requiredPermissionNames="adminSampleTemplatesCustomization"
                                 j:titleKey="sampleBootstrapTemplates.settings"
                                 jcr:mixinTypes="jmix:rbTitle"
                                           jcr:primaryType="jnt:contentTemplate">
                                <j:translation_en jcr:language="en"
                                                  jcr:mixinTypes="mix:title"
                                                  jcr:primaryType="jnt:translation"
                                                  jcr:title="customization"/>
                                <pagecontent jcr:primaryType="jnt:contentList">
                                    <bootstraptemplatecustomization jcr:primaryType="jnt:bootstrapTemplateCustomization"/>
                                </pagecontent>
                            </customization>
                        </site-settings-base>
                </templates>

            <home j:isHomePage="true"
               j:templateName="3col"
                  jcr:primaryType="jnt:page">
                <j:translation_en jcr:language="en"
                                  jcr:mixinTypes="mix:title"
                                  jcr:primaryType="jnt:translation"
                                  jcr:title="Home"/>
            </home>
                <search-results j:templateName="1col" jcr:primaryType="jnt:page">
                    <j:translation_en jcr:language="en"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Search results"/>
                    <pagecontent jcr:primaryType="jnt:contentList">
                        <advanced-search-form jcr:primaryType="jnt:advancedSearchForm"/>
                        <search-results autoSuggest="true"
                               autoSuggestHitCount="2"
                               autoSuggestMaxTermCount="1"
                               autoSuggestMinimumHitCount="2"
                               jcr:primaryType="jnt:searchResults"/>
                    </pagecontent>
                </search-results>
        </sample-bootstrap-templates>
    </modules>
</content>
